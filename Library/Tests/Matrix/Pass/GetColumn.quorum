use Libraries.Compute.Matrix

class Main 
    action Main
        Matrix matrix
        matrix:Fill(2, 3, 1.1)
        Matrix col1 = matrix:GetColumn(1)
        CheckResult(col1:ToText())
    end

    action CheckResult(text result)
        text answer = "|1.1|
|1.1|"
        text lf = answer:GetLineFeed()
        text cr = answer:GetCarriageReturn()
        answer = answer:Replace(cr, lf)
        result = result:Replace(cr, lf)
        if answer = result
            output "pass"
        else
            output "fail"
        end
    end
end